os.loadAPI("SkyNet/MWP/lib/waypoint")

corner1 = vector.new(-114,90,359)
corner2 = vector.new(-178,90,359)
corner3 = vector.new(-178,90,427)
corner4 = vector.new(-114,90,423)

point1 = waypoint.waypoint:new('point1',corner1)
point2 = waypoint.waypoint:new('point2',corner2)
point3 = waypoint.waypoint:new('point3',corner3)
point4 = waypoint.waypoint:new('point4',corner4)

print('Going to tile 1')
point1:goto(1,false)
print('Going to tile 2')
point2:goto(1,false)
print('Going to tile 3')
point3:goto(1,false)
print('Going to tile 4')
point4:goto(1,false)
